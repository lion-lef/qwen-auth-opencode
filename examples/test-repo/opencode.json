{
  "$schema": "https://opencode.ai/config.json",
  "provider": {
    "openai": {
      "name": "OpenAI (Codex)",
      "models": {
        "gpt-5.1-codex": {
          "name": "GPT-5.1 Codex"
        },
        "gpt-5.1-codex-mini": {
          "name": "GPT-5.1 Codex Mini"
        },
        "gpt-5.2-codex": {
          "name": "GPT-5.2 Codex"
        }
      }
    },
    "qwen": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "Qwen (DashScope)",
      "options": {
        "baseURL": "https://dashscope.aliyuncs.com/compatible-mode/v1"
      },
      "models": {
        "qwen-turbo": {
          "name": "Qwen Turbo",
          "limit": {
            "context": 128000
          }
        },
        "qwen-plus": {
          "name": "Qwen Plus",
          "limit": {
            "context": 128000
          }
        },
        "qwen-max": {
          "name": "Qwen Max",
          "limit": {
            "context": 32000
          }
        },
        "qwen-coder-turbo": {
          "name": "Qwen Coder Turbo",
          "limit": {
            "context": 128000
          }
        },
        "qwen-coder-plus": {
          "name": "Qwen Coder Plus",
          "limit": {
            "context": 128000
          }
        },
        "qwen2.5-coder-32b-instruct": {
          "name": "Qwen 2.5 Coder 32B",
          "limit": {
            "context": 32000
          }
        }
      }
    }
  },
  "plugin": [
    "codex",
    "qwen-auth"
  ],
  "agent": {
    "coder": {
      "model": "openai:gpt-5.1-codex",
      "systemPrompt": "You are an expert software developer."
    },
    "task": {
      "model": "qwen:qwen-turbo"
    }
  }
}
