{
  "name": "qwen-auth",
  "version": "1.0.0",
  "description": "Authentication plugin for OpenCode that supports Qwen models with OAuth, JWT, and API key authentication",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "author": "lion-lef",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lion-lef/qwen-auth-opencode.git"
  },
  "homepage": "https://github.com/lion-lef/qwen-auth-opencode#readme",
  "bugs": {
    "url": "https://github.com/lion-lef/qwen-auth-opencode/issues"
  },
  "keywords": [
    "opencode",
    "qwen",
    "alibaba",
    "auth",
    "oauth",
    "jwt",
    "api-key",
    "plugin",
    "authentication"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:plugin": "bun run scripts/build-plugin.ts",
    "build:all": "bun run build && bun run build:plugin",
    "build:schema": "bun run scripts/build-schema.ts",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "prepublishOnly": "bun run build",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@opencode-ai/plugin": "^1.1.35",
    "jose": "^5.2.0",
    "zod": "^3.24.0"
  }
}
